{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "agent-companion",
  "title": "Agent Companion",
  "description": "Manage and orchestrate multiple AI development agents, script hooks, terminal workflows, repo memory, and SuperClaude, all from Raycast.",
  "icon": "extension-icon.png",
  "author": "likesjx",
  "owner": "jared-likes-family",
  "license": "MIT",
  "commands": [
    {
      "name": "view-projects",
      "title": "View Projects",
      "description": "View and manage your projects",
      "mode": "view"
    },
    {
      "name": "manage-claude-code-router",
      "title": "Manage Claude Code Router",
      "description": "Manage the configuration for Claude Code Router",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.101.1",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}